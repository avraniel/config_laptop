alias myconfig="sudo cp -Rfv ~/config_laptop/.config/{plank,thunar,neofetch,openbox,variety,xfce4,ranger,rofi,polybar} ~/.config && cp -Rfv ~/config_laptop/.config/picom.conf ~/.config"

alias reloadbash="sudo cp -Rfv ~/config_laptop/.config/.bashrc ~/"
alias bspwmcp="sudo cp -Rfv ~/config_laptop/.config/bspwmrc ~/.config/bspwm/bspwmrc"
